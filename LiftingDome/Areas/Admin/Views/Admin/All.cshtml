@model IEnumerable<AllUsersViewModel>

@{
	ViewData["Title"] = "All Users";
}
<h2 class="text-center">@ViewData["Title"]</h2>
@if (!Model.Any())
{
	<h2 class="text-center">No users in the system!</h2>
}
else
{
	<div class="row">
		@foreach (AllUsersViewModel user in Model)
		{
			<partial name="_AllUsersViewPartial" model="user" />
		}
	</div>
	<a asp-area="Admin" asp-controller="Home" asp-action="Index" class="btn btn-dark btn-lg">Go Back</a>
}
